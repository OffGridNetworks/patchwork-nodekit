{
  "name": "offgrid-patchwork",
  "version": "1.0.0",
  "description": "web and mobile P2P chat",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/offgridn/offgrid-node"
  },
  "scripts": {
    "desktop": "electron .",
    "clean": "rimraf dist",
    "copy": "npm run clean && mkdir -p ./dist && cp -r ./public dist && cp -r ./static dist",
    "prepublish": "npm run build:ui",
    "build:ui": "mkdir -p ./ui/css && lessc ./ui/less/main.less ./ui/css/main.css",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.js",
    "build:prod": "npm run build:webpack && npm run copy",
    "start": "node server/server",
    "dev": "NODE_ENV=development node server/devServer"
  },
  "author": "OffGridn",
  "license": "APACHE-2.0",
  "devDependencies": {
  "babel-plugin-typecheck": "0.0.3",
    "babel-core": "^5.5.8",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.1.4",
    "babel-plugin-react-transform": "^1.0.5",
    "chai": "^3.0.0",
    "chai-http": "^1.0.0",
    "cookies-js": "^1.2.1",
    "css-loader": "^0.15.1",
    "eslint": "^1.4.3",
    "eslint-config-airbnb": "1.0.0",
    "eslint-plugin-react": "^3.4.0",
    "expect": "^1.10.0",
    "jsdom": "^6.5.0",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.2",
    "mocha-jsdom": "^1.0.0",
    "nock": "^2.18.2",
    "raw-loader": "^0.5.1",
    "react-addons": "^0.9.1-deprecated",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.6",
    "redux-devtools": "^2.1.0",
    "webpack": "^1.9.10",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.9.0",
    "webpack-hot-middleware": "^1.1.0",
    "webpack-isomorphic-tools": "^0.8.8"
  },
  "dependencies": {
    "babel-register": "*",
    "express": "~4.4.5",
    "level-mem": "^0.18.0",
    "options": ">=0.0.5",
    "set-tojson": "^2.0.1",
    "sockjs": "0.3.15",
    "style-loader": "^0.12.4",
    "ultron": "1.0.x"
  }
}
